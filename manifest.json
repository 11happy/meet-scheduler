{
    "name": "Google Meet Links",
    "version": "1.0",
    "description": "Enter your google meet links",
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "css": ["style.css"],
            "js": ["popup.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "permissions": [
        "storage",
        "alarms",
        "tabs",
        "http://*/",
        "https://*/"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}